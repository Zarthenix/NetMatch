@using NetMatch_PT.Models
@model NetMatch_PT.Models.User;
@{
    ViewData["Title"] = "Login";
}
<center>
<h1>Login</h1>

<form asp-action="Login" asp-controller="User" method="get">
    <input type="text" asp-for="@Model.Email" />
    <input type="password" asp-for="@Model.Password" />
    <input type="submit" value="Login" />
</form>
</center>